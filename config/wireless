
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT40'
	option disabled '0'
	option channel '5'
	option txpower '30'
	option country 'US'

config wifi-iface 'ap'
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option network 'lan'
	option macaddr '00:ca:01:0f:00:02'
	option disabled '0'
	option ssid 'Spot_Inspection'

config wifi-iface 'sta'
	option network 'wan'
	option ssid 'HUISHU'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'huishu505'
	option macaddr '00:ca:01:0f:00:01'
	option disabled '1'

